<%= form_for(:session, url: login_path) do |f| %>
    <div>
        <%= f.label :email%>
    </div>
    <div>
        <%= f.text_field :email %>
    </div>
    <div>
        <%= f.label :password %>
    </div>
    <div>
        <%= f.password_field :password %>
    </div>
    <div>
        <%= f.submit "Log in" %>
    </div>
<% end %>